  <metadata>
    <id>@Package.Id</id>
    <version>@Package.Version</version>
    <authors>low-priority</authors>
    <owners>episource</owners>
    
    <title>OllyDumpEx (x64dbg Plugin)</title>
    <summary>A process memory dumper plugin for x64dbg.</summary>
    <tags>admin x64dbg dbg debug debugger reversing</tags>
    
    <!--<iconUrl>https://the.url/</iconUrl>-->
    <projectUrl>http://low-priority.appspot.com/ollydumpex/</projectUrl>
    <!--<projectSourceUrl>https://the.url/</projectSourceUrl>-->
    <!--<bugTrackerUrl>https://the.url/</bugTrackerUrl>-->
    <!--<docsUrl>https://the.url/</docsUrl>-->
    <!--<mailingListUrl>https://the.url/</mailingListUrl>-->
    
    <!--<licenseUrl>https://the.url/</licenseUrl>-->
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <!--<copyright>the copyright</copyright>-->
    
    <description>
 * Various debuggers supported
 * Select to dump debugee exe, loaded dll or non-listed module
 * Search PE File from memory
 * Multiple Dump mode. Rebuild for typical PE dump, Binary for PE Carving
 * PE32+ supported (Search and Binary Dump mode only available on 32bit debugger)
 * Native 64bit process supported (IDA Pro, WinDbg and x64dbg)
 * Dump any address space as section even if not in original section header
 * Add dummy section to keep PE format consistency
 * Fix RVA in DataDirectory to follow ImageBase change
 * Auto calculate many parameters (RawSize, RawOffset, VirtualOffset, ...)
    </description>
    <releaseNotes><![CDATA[@Package.ReleaseNotes]]></releaseNotes>

    <dependencies>
        <dependency id="x64dbg" version="[2017.3.23.1413,)"/>
    </dependencies>
  </metadata>
  <files>
    <file src="tools\**" target="tools" />
  </files>
</package>