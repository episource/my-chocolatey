<?xml version="1.0"?>
<!-- Do not remove this test for UTF-8: if “Ω” doesn’t appear as greek uppercase omega letter enclosed in quotation marks, you should use an editor that supports UTF-8, not this one. -->
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>@Package.Id</id>
    <version>1.0.0</version>
    <title>FileSeek Custom Configuration</title>
    <authors>episource</authors>
    <owners>episource</owners>
    <projectUrl>https://www.fileseek.ca/</projectUrl>
    <bugTrackerUrl>https://www.fileseek.ca/Discussions/</bugTrackerUrl>
    <docsUrl>https://www.fileseek.ca/Help/</docsUrl>
    <iconUrl>https://www.fileseek.ca/ImagesCommon/Icons/64x64/FileSeek.png</iconUrl>
    <licenseUrl>hhttps://www.fileseek.ca/License/</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <summary>My default FileSeek configuration.</summary>
    <description>This configuration package changes the following settings:
 * Disable auto-update
 * Use notepad++ as default file viewer
 * Show an error when an empty PDF file is found
 * Reduce the number of characters shown for each search result from 1000 to 100
</description>
    <releaseNotes>
v1.0.0 - Initial version
    </releaseNotes>
    <tags>fileseek search admin config</tags>
    <dependencies>
        <dependency id="fileseek"          version="[5.1.0,)"/>
        <dependency id="notepadplusplus"   version="[6.9.2,)"/>
        <dependency id="regedit.extension" version="[1.1.0,)"/>
    </dependencies>
  </metadata>
  <files>
    <file src="tools\**" target="tools" />
  </files>
</package>