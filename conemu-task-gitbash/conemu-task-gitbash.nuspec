    </releaseNotes>

    <dependencies>
      <dependency id="conemu"                version="[16.9.4,)"/>
      <dependency id="conemu-config"         version="[3.0.0,)"/>
      <dependency id="git"                   version="[2.10.0,)"/>
      <dependency id="conemu-task.extension" version="[1.0.0,)"/>
    </dependencies>
  </metadata>
  <files>
    <file src="tools\**" target="tools" />
  </files>
